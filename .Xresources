! URxvt color scheme to match gnome-terminal
URxvt*background: #2B2B2B
URxvt*foreground: #DEDEDE
URxvt*font: xft:DejaVuSansMono:size=11
URxvt*boldFont: xft:DejaVuSansMono-Bold:size=11
URxvt*saveLines: 12000
URxvt*scrollBar: false
URxvt*scrollstyle: rxvt
! default,macher: implement clickable URL
! selection-to-clipboard: copy primary to clipboard automatically
URxvt.perl-ext-common: default,matcher,selection-to-clipboard
URxvt*urlLauncher: firefox
URxvt*matcher.button: 1
URxvt*colorUL: #86a2be
